
@using ArteVida.Relatorios
@model ArteVida.GestorWeb.ViewModels.JanelaViewModel

@(Html.TelerikReporting().ReportViewer()
        .Id("reportViewer1")
        .ServiceUrl(Url.Content("~/api/reports/"))
        .TemplateUrl(Url.Content("~/ReportViewer/templates/telerikReportViewerTemplate.html"))
        // .ReportSource(new TypeReportSource() { TypeName = typeof(ListagemBase).AssemblyQualifiedName })
        .ReportSource(
            new UriReportSource() { Uri = @Model.Relatorio }
        )
        .ViewMode(ViewMode.PrintPreview)
        .ScaleMode(ScaleMode.FitPageWidth)
        .Scale(1.0)
)

<script src="~/Scripts/resources.pt-BR.js"></script>